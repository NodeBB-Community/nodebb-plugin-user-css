{
	"id": "nodebb-plugin-user-css",
	"url": "https://github.com/BenLubar/nodebb-plugin-user-css",
	"library": "./index.js",
	"hooks": [
		{
			"hook": "filter:user.customSettings",
			"method": "addCustomSetting"
		},
		{
			"hook": "filter:user.saveSettings",
			"method": "filterUserSaveSettings"
		},
		{
			"hook": "filter:user.getSettings",
			"method": "filterUserGetSettings"
		},
		{
			"hook": "filter:middleware.renderHeader",
			"method": "renderHeader"
		}
	]
}
